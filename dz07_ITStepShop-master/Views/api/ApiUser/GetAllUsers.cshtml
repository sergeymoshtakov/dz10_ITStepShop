@model IEnumerable<ShopUser>

<h1>All Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Full Name</th>
            <th>Delivery Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.FullName</td>
                <td>@user.AddressDelivery</td>
            </tr>
        }
    </tbody>
</table>
